  "private": true,
  "scripts": {
    "dev": "nuxt --port 3000",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "md": "rake update && processmd ./md/*.md --stdout --outputDir contents/posts > contents/posts/summary.json && yarn maketag",
    "deploy": "yarn md && ruby lib/make_tags_json.rb && yarn build && firebase deploy",
    "delcache": "rm -rf node_modules/.cache/hard-source/",
    "new": "ruby lib/make_new_markdown.rb",
    "maketag": "ruby lib/make_tags_json.rb"
  },
  "dependencies": {
    "@nuxtjs/google-adsense": "^1.1.3",
