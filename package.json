    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fstream": "^1.0.12",
    "lodash": "^4.17.13",
    "npm-check-updates": "^3.1.12",
    "nuxt": "^2.8.1",
    "tar": "^4.4.10",
    "vue-burger-menu": "^2.0.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
