    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "processmd": "^4.4.2",
    "sass-loader": "^7.1.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  }
